{{> wrapper_header page_title='Feedback'}}

<div class = "Details">
    <h1 class="carModel">{{car}}</p>
    <h3 class="JobType">Job Type: {{Type}}</h3>
    <h3 class="clientName">Client: {{client}}</h3>
    <h3 class="requestNo">Request No.: {{id}}</h3>
</div>

<div class="container Feedback-container">
    <h4>Message</h4>
    <div id="feedback-container">
        {{#each feedbackcard}}
            {{> feedbackcard}}
        {{/each}}
    </div>
    <input type="Text" class="Feedback_box" placeholder="Add Feedback">

    <a href="/uviewfeedback/{{id}}"><button class="Enter_Feedback" id={{id}}>Send</button></a>
</div>

{{> wrapper_footer}}

